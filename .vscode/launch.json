  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/dist/componentDB.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "sample",
      "program": "${workspaceFolder}/dist/analyzer.js",
      "args": [
        "./sample-project",
        "./out/graph.json"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "preLaunchTask": "build"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "maemoo",
      "program": "${workspaceFolder}/dist/analyzer.js",
      "args": [
        "../maemoo-minibus",
        "./out/graph.json"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "preLaunchTask": "build"
    }
  ]
}